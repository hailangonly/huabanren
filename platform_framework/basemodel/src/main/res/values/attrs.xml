<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomView">
        <attr name="bottom_text" format="string" />
        <attr name="bottom_src" format="color|reference" />
        <attr name="bottom_paddingBetween" format="reference|dimension" />
    </declare-styleable>

    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>
</resources>